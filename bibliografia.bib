@article{INFOCOMP,
	author = {Michel Soares},
	title = { Comparação entre Metodologias Ágeis e Tradicionais para o Desenvolvimento de Software},
	journal = {INFOCOMP},
	volume = {3},
	number = {2},
	year = {2004},
	keywords = {Metodologias de Desenvolvimento, Extreme Programming, Modelo Clássico},
	abstract = {Este artigo faz uma comparação entre as metodologias tradicionais para desenvolvimento de software e as metodologias ágeis. Em particular é feita a comparação da Extreme Programming (XP), uma metodologia ágil muito usada, e o modelo Clássico ou Sequencial. As práticas da XP são apresentadas, enfatizando que suas características são ideais para projetos que devem ter um desenvolvimento rápido e que podem ter requisitos alterados constantemente pelos clientes.},
	issn = {1982-3363},	pages = {8--13},	url = {http://www.dcc.ufla.br/infocomp/index.php/INFOCOMP/article/view/68}
}

